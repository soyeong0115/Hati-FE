{
  "rewrites": [
    {
      "source": "/result",
      "destination": "/api/result",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(?i).*(kakao|facebookexternalhit|twitterbot|linkedinbot|whatsapp|telegrambot|slackbot|bot|crawler|spider).*"
        }
      ]
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
